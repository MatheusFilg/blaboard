model Organization {
  id          String   @id @map("_id")
  name        String
  description String
  userIds     String[]
  users       User[]   @relation(fields: [userIds], references: [id])
  columns     Column[]
  tasks       Task[]
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("organization")
}
